{
    "name": "phpbench/tabular",
    "description": "Generate complex tables from simple configuration",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "~5.4|^7.0",
        "phpbench/dom": "~0.1",
        "seld/jsonlint": "~1.0",
        "justinrainbow/json-schema": "~1.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\Tabular\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBench\\Tabular\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/phpbench"]
}

